<!DOCTYPE html>
<html ng-app="jiujitsmi">
<head>
  <title>GRAPPLEDASH</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
  <!-- for FF, Chrome, Opera -->
<link rel="icon" type="image/png" href="<%= image_path('favicon-16x16.png') %>" sizes="16x16">
<link rel="icon" type="image/png" href="<%= image_path('favicon-32x32.png') %>" sizes="32x32">

<!-- for IE -->
<link rel="icon" type="image/x-icon" href="<%= image_path('favicon.ico') %>" >
<link rel="shortcut icon" type="image/x-icon" href="<%= image_path('favicon.ico') %>"/>

<!-- VIEWPORT FOR RESPONSIVENESS -->
<meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0" />
</head>
<body ng-view>

<%= yield %>

</body>
</html>
